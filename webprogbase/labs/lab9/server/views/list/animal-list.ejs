 <div type="x-tmpl-mustache" id="template-animals-list" class="hidden">
      <div class="carousel-reviews broun-block">
            <div class="container">
                  <div class="row">
                        <div id="carousel-reviews" class="carousel slide" data-ride="carousel">
                              {{#animals}} 
                              <div class="item">
                                    <div class="col-md-4 col-sm-6">
                                          <div class="block-text rel zmin">
                                                <a title="">{{breed}} 
                                                <button type="button" id="remove" value="${{id}}" class="rem" data-toggle="modal" data-target="#myModal">
                                                      <span class="glyphicon glyphicon-remove"></span>
                                                </button>
                                               
                                                </a>
                                                  <div class="mark"> <img  alt=""  class="animal" src="./public/images/{{picture_name}}.{{picture_type}}"></div>                                              
                                                <p>{{description}}</p>
                                                <ins class="ab zmin sprite sprite-i-triangle block"></ins>
                                          </div>
                                          <div class="person-text rel">  
                                                <% if(user) { %>
                                                      <a title="" href="/animals/{{id}}">{{breed}}</a>
                                                <%} else {%>
                                                       <a title="">{{breed}}</a>
                                                <%}%>
                                          </div>
                                    </div> 
                              </div>
                              {{/animals}}
                        </div>
                   </div>
            </div>
      </div>
</div>

  <div class="modal fade text-left" id="myModal" role="dialog">
       <div class="modal-dialog">
            <div class="modal-content">
                  <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title">Delete </h4>
                  </div>
                  <div class="modal-body">
                  <p>Are you sure?</p>
                  </div>
                  <div class="modal-footer">
                  <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                  <button id="remove_el" type="button" class="btn btn-primary">Delete</button>
                  </div>
            </div>
      </div>
</div>   